{
  "root": true,
  "ignorePatterns": [
    "node_modules",
    "index.js",
    "index.d.ts",
    "lib",
    "dist"
  ],
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-console": "off"
  }
}